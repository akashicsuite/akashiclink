{
  "name": "@helium-pay/wallet-extension",
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "serve": "serve -d build",
    "build": "GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true craco build",
    "build:dev": "watch 'DISABLE_ESLINT_PLUGIN=true env-cmd -f .env craco build' ./src",
    "build:staging": "yarn lint:fix && env-cmd -f .env.staging.local craco build",
    "build:prod": "yarn lint:fix && env-cmd -f .env.production.local craco build",
    "sync:device": "yarn build:staging && env-cmd -f .env.staging.local npx cap sync",
    "sync:prod:device": "yarn build:prod && env-cmd -f .env.production.local npx cap sync",
    "build:android": "yarn updateVersion:android && PLATFORM=android yarn sync:device && cd android && bundle exec fastlane buildApk --env staging",
    "build:prod:android": "yarn updateVersion:prod:android && PLATFORM=android yarn sync:prod:device && cd android && bundle exec fastlane buildApk --env production",
    "build:ios": "yarn updateVersion:ios && PLATFORM=ios yarn sync:device && cd ios/App && bundle exec fastlane buildIpa --env staging",
    "build:prod:ios": "yarn updateVersion:prod:ios && PLATFORM=ios yarn sync:prod:device && cd ios/App && bundle exec fastlane buildIpa --env production",
    "deploy:android": "yarn updateVersion:android && PLATFORM=android yarn sync:device && cd android && bundle exec fastlane internal --env staging",
    "deploy:prod:android": "yarn updateVersion:prod:android && PLATFORM=android yarn sync:prod:device && cd android && bundle exec fastlane internal --env production",
    "deploy:ios": "yarn updateVersion:ios && PLATFORM=ios yarn sync:device && cd ios/App && bundle exec fastlane internal --env staging",
    "deploy:prod:ios": "yarn updateVersion:prod:ios && PLATFORM=ios yarn sync:prod:device && cd ios/App && bundle exec fastlane internal --env production",
    "serve:android": "npx cap open android",
    "serve:ios": "npx cap open ios",
    "deploy:extension": "yarn build:staging && cd extension && env-cmd -f .env.staging node deploy.mjs",
    "deploy:prod:extension": "yarn build:prod && cd extension && env-cmd -f .env.production node deploy.mjs",
    "updateVersion:android": "cd android && bundle exec fastlane updateManifest --env staging",
    "updateVersion:prod:android": "cd android && bundle exec fastlane updateManifest --env production",
    "updateVersion:ios": "cd ios/App && bundle exec fastlane updateManifest --env staging",
    "updateVersion:prod:ios": "cd ios/App && bundle exec fastlane updateManifest --env production",
    "start:dev": "DISABLE_ESLINT_PLUGIN=true PORT=3001 craco start",
    "lint": "npx eslint . --cache",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "REACT_APP_API_BASE_URL=http://localhost:6006/api/v0 DISABLE_ESLINT_PLUGIN=true storybook dev -p 6006 -c ./storybook",
    "test-storybook": "TARGET_URL=http://localhost:6006 test-storybook --config-dir storybook --verbose",
    "test-coverage": "yarn test-storybook --coverage",
    "ionic:build": "craco build",
    "ionic:serve": "craco start",
    "debug:android": "env-cmd -f .env.staging.local ionic cap run android -l --external",
    "debug:ios": "env-cmd -f .env.staging.local ionic cap run ios -l --external",
    "analyze-bundle": "craco build && source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@activeledger/activecrypto": "^2.12.3",
    "@activeledger/sdk-bip39": "1.0.3",
